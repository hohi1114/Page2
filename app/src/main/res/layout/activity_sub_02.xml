
<ScrollView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        tools:context=".SubActivity_02">

        <ImageView
            android:id="@+id/imageView0"
            android:layout_width="92dp"
            android:layout_height="85dp"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="40dp"
            android:contentDescription="TODO"
            android:src="@drawable/music" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="92dp"
            android:layout_height="85dp"
            android:layout_below="@+id/imageView0"
            android:src="@drawable/blueming" />
        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="92dp"
            android:layout_height="85dp"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/imageView1"
            android:src="@drawable/four" />
        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="92dp"
            android:layout_height="85dp"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/imageView2"
            android:src="@drawable/snow" />
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="92dp"
            android:layout_height="85dp"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/imageView3"
            android:src="@drawable/return1"
            android:contentDescription="TODO" />



        <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="126dp"
        android:layout_marginTop="144dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="20dp"
        android:layout_toRightOf="@+id/imageView1"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/start"
        android:background="?attr/selectableItemBackground"
        />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="144dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageButton1"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/stop"
            android:background="?attr/selectableItemBackground"
            />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/imageButton1"
            android:layout_marginLeft="126dp"
            android:layout_marginTop="64dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageView2"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/start"
            android:background="?attr/selectableItemBackground"
            />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="50dp"
            android:layout_height="50dp"

            android:layout_below="@+id/imageButton2"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="64dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageButton3"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/stop"
            android:background="?attr/selectableItemBackground"
            />
        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/imageButton3"
            android:layout_marginLeft="126dp"
            android:layout_marginTop="64dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageView3"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/start"
            android:background="?attr/selectableItemBackground"
            />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/imageButton4"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="64dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageButton5"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/stop"
            android:background="?attr/selectableItemBackground"
            />
        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/imageButton5"
            android:layout_marginLeft="126dp"
            android:layout_marginTop="64dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageView4"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/start"
            android:background="?attr/selectableItemBackground"
            />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@+id/imageButton6"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="64dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:layout_toRightOf="@+id/imageButton7"
            android:padding="0dp"
            android:scaleType="fitCenter"
            android:src="@drawable/stop"
            android:background="?attr/selectableItemBackground"
            />

        <Button
            android:id="@+id/prev_page2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageButton2"
            android:layout_marginTop="430dp"
            android:text="@string/back"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />
        <Button
            android:id="@+id/prev_page3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageButton2"
            android:layout_marginTop="600dp"
            android:text="@string/back"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />
        <View
            android:layout_width="match_parent"
            android:layout_below="@+id/prev_page3"
            android:layout_height="25dp"/>

    </RelativeLayout>
    </LinearLayout>
</ScrollView>


